<div class="container">
  <div class="title">
    <h1>Browse through the questions</h1>

  </div>
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <ul>
      {{#each model.posts as |post|}}
      {{post-tile post=post}}
      {{/each}}



      </ul>

    </div>
    <div class="col-md-2">
      {{new-post savePost="savePost" }}
    </div>

  </div>

</div>
